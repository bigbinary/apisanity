{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion":6,
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6":true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "connect": true,
    "process":true,
    "require":true,
  },
  "rules": {
    "func-names": 0,
    "curly": 0,
    "no-else-return": 0,
    "array-bracket-spacing": 0,
    "object-shorthand": 0,
    "consistent-return": 0,
    "prefer-template": 0,
    "no-use-before-define": 0,
    "strict": 0,
    "max-len": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "no-console": 0,
    "id-length": 0,
    "no-unused-expressions": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "react/prop-types": 0,
    "no-param-reassign": 0,
    "react/jsx-first-prop-new-line": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "arrow-body-style": [0, "as-needed"],
    "react/jsx-no-bind": 0
  },
  "plugins": [
    "react"
  ]
}
